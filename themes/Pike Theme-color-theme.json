{
	"name": "Pike Theme",
	"type": "dark",
	"colors": {
		// 活动栏开始
		"activityBar.background": "#3a3b3ae1",
		"activityBar.foreground": "#e0dfdf",
		"activityBarBadge.background": "#cc7c4d",
		"activityBarBadge.foreground": "#f8fafd",
		// 活动栏结束
		"badge.background": "#737579", // 小型信息框, 如git提交
		"button.background": "#636568", // 按钮背景色
		"debugToolBar.background": "#363638", // 调试工具栏背景色
		"diffEditor.insertedTextBackground": "#00809b33", // 已插入的文本的背景色 
		"dropdown.background": "#53504f", // 下拉列表背景色
		"dropdown.border": "#21252b", // / 下拉列表边框
		"editor.background": "#2d2e30", // 背景色
		"editor.findMatchBackground": "#7e630b2f", // 当前搜索匹配项的颜色
		"editor.findMatchBorder": "#99900f80", // 搜索匹配项边框
		"editor.findMatchHighlightBackground": "#7e630b2f", // 搜索匹配项
		"editor.foreground": "#d7d9dd", // 括号颜色
		"editor.lineHighlightBackground": "#6b68671e", // 光标所在行高亮内容的背景颜色。
		"editor.selectionBackground": "#ffffff1f", // 编辑器所选内容的颜色
		"editor.selectionHighlightBackground": "#ffffff10", // 具有与所选项相关内容的区域的颜色
		"editor.selectionHighlightBorder": "#dddddd56", // 与所选项内容相同的区域的边框颜色
		"editor.wordHighlightBackground": "#d2e0ff2f",
		"editor.wordHighlightBorder": "#7f848e",
		"editor.wordHighlightStrongBackground": "#abb2bf26",
		"editor.wordHighlightStrongBorder": "#7f848e",
		"editorBracketMatch.background": "#515a6b",
		"editorBracketMatch.border": "#515a6b",

		"editorCursor.background": "#ffffffc9", // 光标背景色
		"editorCursor.foreground": "#ff9252", // 光标颜色
		"editorError.foreground": "#c24038", // 错误波浪线
		"editorGroup.border": "#ffffff",
		"editorGroupHeader.tabsBackground": "#252526", // 启用选项卡时编辑器组标题的背景颜色。编辑器组是编辑器的容器
		"editorHoverWidget.background": "#333333", // 编辑器悬停提示的背景颜色
		"editorHoverWidget.border": "#464646", // 光标悬停时编辑器的边框颜色
		"editorIndentGuide.activeBackground": "#c8c8c859", // 编辑器活动缩进参考线的颜色
		"editorIndentGuide.background": "#3b4048", // 编辑器缩进参考线的颜色。
		"editorLineNumber.foreground": "#cacaca", // 编辑器行号的颜色。
		"editorMarkerNavigation.background": "#21252b", // 编辑器标记导航小组件背景色
		"editorRuler.foreground": "#abb2bf26", // 编辑器标尺的颜色
		"editorSuggestWidget.background": "#21252b", // 建议小组件的背景色
		"editorSuggestWidget.border": "#181a1f", // 建议小组件的边框颜色
		"editorSuggestWidget.selectedBackground": "#2c313a", // 建议小组件中所选条目的背景色
		"editorWarning.foreground": "#d19a66", // 编辑器中警告波浪线的前景色
		"editorWhitespace.foreground": "#3b4048", // 编辑器中空白字符的颜色
		"editorWidget.background": "#535558", // 编辑器组件(如查找/替换)背景颜色。
		"focusBorder": "#a0a0a0", // 焦点元素的整体边框颜色。此颜色仅在不被其他组件覆盖时适用
		"input.background": "#3c3c3d", // 输入框背景色
		// 左侧树状选中的效果
		"list.activeSelectionBackground": "#4a4b4d",
		"list.activeSelectionForeground": "#d7dae0",
		"list.focusBackground": "#4a4b4d",
		"list.highlightForeground": "#c5c5c5",
		"list.hoverBackground": "#4a4b4d",
		"list.inactiveSelectionBackground": "#4a4b4d",
		"list.inactiveSelectionForeground": "#d7dae0",
		"list.warningForeground": "#d19a66",
		"menu.foreground": "#c8c8c8",
		"peekViewEditor.background": "#5c5d5e",
		"peekViewEditor.matchHighlightBackground": "#757577",
		"peekViewResult.background": "#63676b",
		// "scrollbarSlider.activeBackground": "#747d9180",
		// "scrollbarSlider.background": "#4e566660",
		// "scrollbarSlider.hoverBackground": "#5a637580",
		// 侧边栏
		"sideBar.background": "#413f3f",
		"sideBarSectionHeader.background": "#505050",
		// 侧边栏

		// 状态栏
		"statusBar.background": "#504f4f", // 工作区打开时状态栏的背景色
